 \n}\n\n.navbar-closed {\n  opacity: 0;\n  width: 0;\n  height: 0; \n  visibility: hidden;\n  font-family: 'Noto Sans TC', sans-serif;  \n}\n\n\n\n#menu-close-btn-container {\n  display: flex;\n  justify-content: flex-end;\n  padding-right: 2rem;\n}\n\n#menu-close-btn {\n   border: none;\n   font-size: 3rem;\n   color: #fff;\n   cursor: pointer;\n}\n\n\n.navbar-link {\n  display: flex;\n  align-items: center;\n  font-size: 1.5rem;\n  line-height: 3rem;\n  justify-content: start;\n  text-decoration: none;\n  color: #fff;\n}\n\n \n\n\n/* fix look of menu */\n.menu-lines {\n  color: #eee;\n  opacity: 0.6;\n  margin-bottom: 0;\n  margin-top: 0;\n}\n.navbar-link:hover {\n  background-color: #333;\n}\n\n.material-icons {\n  margin-right: 2rem;\n  margin-left: 1rem;\n}\n\n  .top-heading {\n    color: #FF5F6D;\n    text-align: center;\n    font-size: 3rem;\n  }\n  \n  /* Everything but the jumbotron gets side spacing for mobile first views */\n  .header,\n  .marketing,\n  .footer {\n    padding-right: 1rem;\n    padding-left: 1rem;\n  }\n  \n  /* Custom page header */\n  .header {\n    padding-bottom: 1rem;\n    border-bottom: .05rem solid #e5e5e5;\n  }\n  /* Make the masthead heading the same height as the navigation */\n  .header h3 {\n    margin-top: 0;\n    margin-bottom: 0;\n    line-height: 3rem;\n  }\n  \n  /* Custom page footer */\n  .footer {\n    padding-top: 1.5rem;\n    color: #777;\n    border-top: .05rem solid #e5e5e5;\n  }\n  \n  /* Customize container */\n  @media (min-width: 48em) {\n    .container {\n      max-width: 46rem;\n    }\n  }\n  .container-narrow > hr {\n    margin: 2rem 0;\n  }\n  \n  /* Main marketing message and sign up button */\n  .jumbotron {\n    text-align: center;\n    border-bottom: .05rem solid #e5e5e5;\n    color: white;\n    border-radius: 20px;\n    padding-left: 0;\n    padding-right: 0;\n    padding-top: 0;\n    \n  }\n  .jumbotron .btn {\n    padding: .75rem 1.5rem;\n    font-size: 1.5rem;\n    background-color: #222;    \n    border-radius: 20px;\n    border-color: #2b2e88;\n  }\n\n  .btn {\n    margin-top: 2rem;\n  }\n\n  #flex-container {\n    display: flex;\n    flex-direction: row;\n    justify-content: space-between;\n  }\n\n  #jumbo-heading {\n    border: 1px solid black;\n    border-radius: 20px 20px 0 0;\n    padding: 1rem 1rem 0 1rem;\n    margin: 0;\n    border-bottom: none;\n    border-left: none;\n    border-right: none;\n  }\n\n  #form {\n    padding-left: 5vh;\n    padding-right: 5vh;\n  }\n\n  #jumbo-bottom {\n    border-radius: 0 0 20px 20px;\n  }\n\n  .website-heading {\n    display: flex;\n    flex-direction: row;\n    justify-content: flex-start;\n    align-items: baseline;\n    padding-left: 5vh;\n  }\n\n.website-name {\n  padding-right: 5vh;\n}\n  \n  /* Supporting marketing content */\n  .marketing {\n    margin: 3rem 0;\n  }\n  .marketing p + h4 {\n    margin-top: 1.5rem;\n  }\n  \n  /* Responsive: Portrait tablets and up */\n  @media screen and (min-width: 48em) {\n    /* Remove the padding we set earlier */\n    .header,\n    .marketing,\n    .footer {\n      padding-right: 0;\n      padding-left: 0;\n    }\n    /* Space out the masthead */\n    .header {\n      margin-bottom: 2rem;\n      margin-top: 5vh;\n    }\n    /* Remove the bottom border on the jumbotron for visual effect */\n    .jumbotron {\n      border-bottom: 5vh;\n      border-color: black;\n    }\n  }\n  "},"lineCount":null}},"error":null,"hash":"d7308ca960bcab0f91576216e93392e7","cacheData":{"env":{}}}